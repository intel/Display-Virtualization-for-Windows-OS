#include <winver.h>

VS_VERSION_INFO VERSIONINFO
FILEVERSION ${FileVersion}
PRODUCTVERSION ${ProductVersion}
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904B0"
        BEGIN
            VALUE "FileVersion", "${FileVersion}"
            VALUE "ProductVersion", "${ProductVersionString}"
            VALUE "ProductName", "DVServer.dll"     
	    VALUE "LegalCopyright", "Copyright (C) 2021 Intel Corporation"
            VALUE "FileDescription", "Display Virtualization User Mode Driver " 
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
